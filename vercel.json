{
  "buildCommand": "npm run build",
  "outputDirectory": "./dist",
  "functions": {
    "api/developments.ts": {
      "runtime": "@vercel/node"
    },
    "api/contact.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/_api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
